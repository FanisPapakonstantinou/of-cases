FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 -1 0 0 0 0 ];

internalField   uniform 1e-5;

boundaryField
{
    inlet
    {
        type           fixedValue;
        value          $internalField;
    }
    outlet
    {
       type            inletOutlet;
       inletValue      $internalField;
       value           $internalField;
    }
    
    symmetry
    {
        type           symmetryPlane;
    }
    
    slipWalls
    {
        type            slip;
    }
    
    wallAhmed
    {
        type           fixedValue;
        value          $internalField;
    }
    
    wallGround
    {
        type           fixedValue;
        value          $internalField;
    }
}


// ************************************************************************* //
